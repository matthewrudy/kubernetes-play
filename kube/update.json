{
   "kind":"ReplicationController",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"go-env",
      "labels":{
         "name":"go-env"
      }
   },
   "spec":{
      "replicas":2,
      "selector":{
         "name":"go-env"
      },
      "template":{
         "metadata":{
            "labels":{
               "name":"go-env"
            }
         },
         "spec":{
            "containers":[
               {
                  "image":"matthewrudy/go-env:v2",
                  "name":"go-env",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":8080,
                        "protocol":"TCP"
                     }
                  ]
               }
            ]
         }
      }
   }
}
