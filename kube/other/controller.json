{
   "kind":"ReplicationController",
   "apiVersion":"v1beta3",
   "metadata":{
      "name":"other",
      "labels":{
         "name":"other"
      }
   },
   "spec":{
      "replicas":1,
      "selector":{
         "name":"other"
      },
      "template":{
         "metadata":{
            "labels":{
               "name":"other"
            }
         },
         "spec":{
            "containers":[
               {
                  "image":"nginx",
                  "name":"nginx",
                  "ports":[
                     {
                        "name":"http-server",
                        "containerPort":80,
                        "protocol":"TCP"
                     }
                  ]
               }
            ]
         }
      }
   }
}
